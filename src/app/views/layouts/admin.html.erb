        </li>
        <li class="treeview">
          <a href="#">
            <i class="fa fa-venus"></i> <span>Promulher</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <li>
              <%= link_to '' do  %>
                <i class="fa fa-circle-o"></i> Novo cadastro
              <% end %>
            </li>
            <li><a href="../../index2.html"><i class="fa fa-circle-o"></i> Listar participantes</a></li>
          </ul>
        </li>
        <li class="treeview">
          <a href="#">
            <i class="fa fa-file-text-o"></i>
            <span>Atividades</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <li>
              <%= link_to new_activity_path do %>
                <i class="fa fa-circle-o"></i> Nova Atividade 
              <% end %>
            </li>
            <li>
              <%= link_to activities_path do %>
                <i class="fa fa-circle-o"></i> Listar atividades
              <% end %>
            </li>
          
          </ul>
        </li>
        <li class="treeview">
          <a href="#">
            <i class="fa fa-bookmark"></i>
            <span>Voluntários</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
