          <ul class="treeview-menu">
            <li><a href="../UI/general.html"><i class="fa fa-circle-o"></i> Cadastrar voluntário</a></li>
            <li><a href="../UI/icons.html"><i class="fa fa-circle-o"></i> Listar voluntários</a></li>
          </ul>
        </li>
        <li class="treeview">
          <a href="#">
            <i class="fa fa-bullhorn"></i> <span>Eventos</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <li>
              <%= link_to new_event_path do %>
                <i class="fa fa-circle-o"></i> Novo Evento
              <% end %>
            </li>
            <li>
              <%= link_to events_path do %>
                <i class="fa fa-circle-o"></i> Listar Eventos
              <% end %>
            </li>
          </ul>
        </li>
        <li>
          <%= link_to calendar_path do %>
            <i class="fa fa-calendar"></i> <span>Calendário</span>
            <span class="pull-right-container">
              <small class="label pull-right bg-primary"><%= events_count %></small>
            </span>
          <% end %>
        </li>
        
        <li><a href="https://adminlte.io/docs"><i class="fa fa-book"></i> <span>Documentação</span></a></li>
        <% if current_user.superadmin? %>
        <li class="header">SUPERADMIN</li>
        <li>
          <%= link_to users_path do %>
            <i class="fa fa-user"></i> <span>Gerenciar Usuários</span>
            <% end %>
        </li>
        <% end %>
      </ul>
