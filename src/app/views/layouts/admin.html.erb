<html style="height: auto; min-height: 100%;"><head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>Medianeiras Admin | <%= title %></title>

  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <%= stylesheet_link_tag 'admin' %>
  <%= javascript_include_tag 'admin' %>
  <%= custom_flash_messages %>
</head>
<body class="hold-transition fixed skin-blue sidebar-mini" style="height: auto; min-height: 100%;">

<!-- Site wrapper -->
<div class="wrapper" style="height: auto; min-height: 100%;">

  <header class="main-header">
    <!-- Logo -->
    <%= link_to admin_path, class: 'logo' do %>
      <!-- mini logo for sidebar mini 50x50 pixels -->
      <span class="logo-mini">
        <%= image_tag "https://github.com/andreaflether/projeto_medianeiras/blob/master/app/assets/images/favicon.png?raw=true"%>
        </span>
      <!-- logo for regular state and mobile devices -->
      <span class="logo-lg">Medianeiras <b>Admin</b></span>
    <% end %>

    <!-- Header Navbar: style can be found in header.less -->
    <nav class="navbar navbar-static-top">
      <!-- Sidebar toggle button-->
      <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </a>

      <div class="navbar-custom-menu">
        <ul class="nav navbar-nav">
          <!-- User Account: style can be found in dropdown.less -->
          <li class="dropdown user user-menu">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              <%= gravatar_image_tag(current_user.email.gsub('spam', 'mdeering'), 
                alt: 'Avatar de usuário', 
                class: 'user-image')
              %>
              <span class="hidden-xs"><%= current_user.full_name %></span>
            </a>
            <ul class="dropdown-menu">
              <!-- User image -->
              <li class="user-header">
                <%= gravatar_image_tag(current_user.email.gsub('spam', 'mdeering'), 
                  alt: 'Avatar de usuário', 
                  class: 'img-circle')
                %>
                <p>
                  <%= current_user.full_name %>
                  <small>Membro desde <%= member_since(current_user.created_at) %></small>
                </p>
              </li>
              <!-- Menu Footer-->
              <li class="user-footer">
                <div class="text-center">
                  <%= link_to 'Preferências de Usuário', edit_user_registration_path,
                  class: 'btn btn-default btn-flat' %>
                  <%# <a href="#" class="btn btn-default btn-flat">Preferências de usuário</a> %>
                </div>
              </li>
            </ul>
          </li>
          <li class="user-menu">
            <%= link_to destroy_user_session_path, method: :delete do %>
              <i class="fa fa-sign-out"></i>&nbsp;Sair
            <% end %>
          </li>
        
        </ul>
      </div>
    </nav>
  </header>

